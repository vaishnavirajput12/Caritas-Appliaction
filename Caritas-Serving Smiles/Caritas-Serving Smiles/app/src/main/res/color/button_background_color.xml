<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--When Button is not enabled -->
    <item android:state_enabled="false" android:color="@color/white" />
    <!--When Button is in pressed state -->
    <item android:state_pressed="true" android:color="@color/white" />
    <!--When Button is in selected state -->
    <item android:state_selected="true" android:color="@color/white" />
    <!--Default Background Color -->
    <item android:color="#97D9A7" />
    <item android:color="#9AA59F" />
    <item android:color="#E1FFE3" />
    <item android:color="#14AE5C" />

</selector>